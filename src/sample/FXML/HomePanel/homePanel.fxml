<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" depthTest="ENABLE" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FXML.HomePanel.HomeController">
   <children>
      <TabPane tabClosingPolicy="UNAVAILABLE" tabMinHeight="50.0" tabMinWidth="300.0" VBox.vgrow="ALWAYS">
         <tabs>
            <Tab text="Find Organization">
               <content>
                  <VBox>
                     <children>
                        <fx:include prefHeight="600.0" prefWidth="1000.0" source="tab1FindOrganizationPanel.fxml" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab text="Find Events">
               <content>
                  <VBox prefHeight="603.0" prefWidth="890.0">
                     <children>
                        <fx:include source="tab2FindEventsPanel.fxml" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </content>
            </Tab>
            <Tab text="Find Volunteer">
               <content>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <fx:include source="tab3FindVolunteer.fxml" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </content>
            </Tab>
         </tabs>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </padding>
      </TabPane>
   </children>
</VBox>
